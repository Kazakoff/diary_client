{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/app/main.js -o dist/build.js",
    "serve": "http-server -o -s -c 1 -a localhost",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/app/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "install": "^0.10.1",
    "underscore": "^1.8.3",
    "vue": "^2.2.6",
    "vue-charts": "^0.2.1",
    "vue-resource": "^1.2.1",
    "vue-router": "^2.3.0",
    "vuex": "^2.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^3.2.4",
    "envify": "^4.0.0",
    "http-server": "^0.9.0",
    "npm-run-all": "^4.0.2",
    "uglify-js": "^2.8.16",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  }
}
